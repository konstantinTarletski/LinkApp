<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions targetNamespace="urn:Processes_BASE/processes/linkapp/sopupdate/LAPP_SOPUpdatePrcWS" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:tns="urn:Processes_BASE/processes/linkapp/sopupdate/LAPP_SOPUpdatePrcWS" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:dlsopiulv="http://dnb.lv/dnb-xst/dnb-linkapp-SOPInformationUpdate-LV" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/">
    <wsdl:types>
        <xsd:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://dnb.lv/dnb-xst/dnb-linkapp-SOPInformationUpdate-LV" xmlns="http://dnb.lv/dnb-xst/dnb-linkapp-SOPInformationUpdate-LV" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <xsd:annotation>
                <xsd:documentation>LV shema for SOPInformationUpdate
